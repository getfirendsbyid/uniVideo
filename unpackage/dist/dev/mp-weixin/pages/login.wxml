<view class="container"><view class="app_wrap"><view class="bg1" style="{{'background-image:'+('url('+loginBg+')')+';'}}"></view><view class="bg2" style="{{'background-image:'+('url('+loginBg2+')')+';'}}"></view><view class="loginBox"><text class="loginText">Welcome</text><view class="login_item"><text>账号</text><input type="text" placeholder="请输入您的账号" placeholder-style="color:#999" maxlength="20" data-event-opts="{{[['input',[['__set_model',['$0','loginAccount','$event',[]],['form']]]]]}}" value="{{form.loginAccount}}" bindinput="__e"/></view><view class="login_item"><text>密码</text><input type="text" placeholder="请输入您的密码" password="true" placeholder-style="color:#999" maxlength="12" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['form']]]]]}}" value="{{form.password}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['goPage',['$event']]]]]}}" class="login_item login_btn" bindtap="__e">登录</view></view></view></view>